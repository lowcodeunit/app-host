{
  "name": "@lowcodeunit/app-host",
  "version": "0.0.2",
  "scripts": {
    "build:app-host": "ng build app-host --configuration production",
    "build:common": "ng build common --configuration production",
    "build:demo": "ng build demo --configuration production",
    "build:lcu": "ng build lcu --configuration production && npm run pack",
    "demo": "npm run build:common && npm run start:demo",
    "demo:lcu": "npm run build:common && npm run build:lcu && npm run start:demo",
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run deploy:common && npm run deploy:lcu && npm run deploy:demo && npm run deploy:app-host",
    "deploy:app-host": "npm version patch --prefix projects/app-host && npm run build:app-host && npm publish ./dist/app-host --access public",
    "deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
    "deploy:demo": "npm version patch --prefix projects/demo && npm run build:demo && npm publish ./dist/demo --access public",
    "deploy:lcu": "npm version patch --prefix projects/lcu && npm run build:lcu && npm publish ./dist/lcu --access public",
    "ng": "ng",
    "pack": "npm run pack:lcu",
    "pack:join": "concat-glob-cli -f \"dist/lcu/wc/lcu.*.js\" -o dist/lcu/wc/app-host.lcu.js",
    "pack:lcu": "rimraf dist/lcu/wc && mkdirp dist/lcu/wc && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
    "pack:main": "concat-glob-cli -f \"dist/lcu/main-es2015.*.js\" -o dist/lcu/wc/lcu.startup.js",
    "pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/lcu/wc/lcu.pollyfills.js",
    "start": "npm run start:app-host",
    "start:app-host": "ng serve app-host --port=4268 --aot=false",
    "start:demo": "ng serve demo --port=4267 --aot=false",
    "build": "npm run build:common && npm run build:lcu",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/elements": "^13.1.0",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@aspnet/signalr": "^1.1.4",
    "@lcu/common": "1.84.755-angular-13",
    "@lowcodeunit-devkit/ng-lcu": "^1.28.161",
    "@lowcodeunit/data-grid": "1.20.257-angular-13",
    "@lowcodeunit/lazy-element": "1.21.85-angular-13",
    "@lowcodeunit/lcu-documentation-common": "1.6.59-angular-13",
    "@lowcodeunit/lcu-icons-common": "1.2.77-angular-13",
    "@lowcodeunit/lcu-theme-builder-common": "1.5.103-angular-13",
    "@webcomponents/custom-elements": "^1.5.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "esbuild": "^0.14.10",
    "ngx-build-plus": "^13.0.1",
    "ngx-color-picker": "^11.0.0",
    "ngx-markdown": "^13.0.0",
    "rxjs": "~7.5.1",
    "sass.js": "^0.11.1",
    "swagger-ui": "^4.1.3",
    "swagger-ui-themes": "^3.0.1",
    "terser": "3.14.1",
    "tinycolor2": "^1.4.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@lcu/cli": "^1.7.97",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.8",
    "@types/tinycolor2": "^1.4.3",
    "concat-glob-cli": "^0.1.0",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^13.1.2",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~10.4.0",
    "tslint": "~6.1.3",
    "typescript": "~4.5.4"
  }
}
