{
  "name": "@lowcodeunit/app-host",
  "version": "0.0.1",
  "scripts": {
    "build:app-host": "ng build app-host --prod",
    "build:common": "ng build common",
    "build:demo": "ng build demo --prod",
    "build:lcu": "ng build lcu --prod --single-bundle && npm run pack",
    "demo": "npm run build:common && npm run start:demo",
    "demo:lcu": "npm run build:common && npm run build:lcu && npm run start:demo",
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run deploy:common && npm run deploy:lcu && npm run deploy:demo && npm run deploy:app-host",
    "deploy:app-host": "npm version patch --prefix projects/app-host && npm run build:app-host && npm publish ./dist/app-host --access public",
    "deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
    "deploy:demo": "npm version patch --prefix projects/demo && npm run build:demo && npm publish ./dist/demo --access public",
    "deploy:lcu": "npm version patch --prefix projects/lcu && npm run build:lcu && npm publish ./dist/lcu --access public",
    "ng": "ng",
    "pack": "npm run pack:lcu",
    "pack:join": "concat-glob-cli -f \"dist/lcu/wc/lcu.*.js\" -o dist/lcu/wc/app-host.lcu.js",
    "pack:lcu": "rimraf dist/lcu/wc && mkdirp dist/lcu/wc && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
    "pack:main": "concat-glob-cli -f \"dist/lcu/main-es2015.*.js\" -o dist/lcu/wc/lcu.startup.js",
    "pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/lcu/wc/lcu.pollyfills.js",
    "start": "npm run start:app-host",
    "start:app-host": "ng serve app-host --port=4268 --aot=false",
    "start:demo": "ng serve demo --port=4267 --aot=false",
    "build": "npm run build:common && npm run build:lcu",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~11.1.2",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "~11.1.2",
    "@angular/compiler": "~11.1.2",
    "@angular/core": "~11.1.2",
    "@angular/elements": "^11.1.2",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.1.2",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "~11.1.2",
    "@angular/platform-browser-dynamic": "~11.1.2",
    "@angular/router": "~11.1.2",
    "@aspnet/signalr": "^1.1.4",
    "@lcu/common": "^1.77.686",
    "@lowcodeunit-devkit/ng-lcu": "^1.26.150",
    "@lowcodeunit/data-grid": "^1.18.201",
    "@lowcodeunit/lazy-element": "^1.20.64",
    "@lowcodeunit/lcu-documentation-common": "^1.5.34",
    "@webcomponents/custom-elements": "^1.4.3",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "ngx-build-plus": "^11.0.0",
    "ngx-monaco-editor": "^9.0.0",
    "rxjs": "~6.6.0",
    "swagger-ui": "^3.43.0",
    "swagger-ui-themes": "^3.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "~11.1.4",
    "@angular/compiler-cli": "~11.1.2",
    "@lcu/cli": "^1.7.97",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concat-glob-cli": "^0.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^11.2.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "terser": "^5.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
